<!DOCTYPE html>
<html>
  <head>
    <title>Resource Wall</title>
    <% include ../partials/header %>
  </head>
  <body>
    <h1> Featured Resources </h1>
    <ul>
      <% resources.forEach((resource) => { %>
        <div class="resource">
          <h2><%= resource.title %></h2>
          <p><%= resource.description %></p>
          <p><%= resource.url %></p>
          <p>Ratings: <%= resource.ratings %></p>
          <p>Likes: <%= resource.likes %></p>
          <p>Comments: <%= resource.comments.length %></p>
        </div>
      <% }) %>
    </ul>
  </body>
</html>
